{
  "name": "vyoke.com",
  "version": "1.0.0",
  "description": "vyoke.com",
  "author": "laure",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build:prod": "cross-env NODE_ENV=production nuxt build && cross-env NODE_ENV=production backpack build",
    "build:test": "cross-env NODE_ENV=test nuxt build && cross-env NODE_ENV=test backpack build",
    "build:pre": "cross-env NODE_ENV=pre nuxt build && cross-env NODE_ENV=pre backpack build",
    "start:prod": "cross-env NODE_ENV=production pm2 start ecosystem.config.js --env production",
    "start:test": "cross-env NODE_ENV=test pm2 start ecosystem.config.js --env test",
    "start:pre": "cross-env NODE_ENV=pre pm2 start ecosystem.config.js --env pre",
    "publish:prod": "npm run build:prod && npm run start:prod",
    "publish:test": "npm run build:test && npm run start:test",
    "publish:pre": "npm run build:test && npm run start:pre",
    "generate": "nuxt generate",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev-debug": "node node_modules/.bin/backpack dev --inspect-brk",
    "test": "nuxt build && ava --verbose --serial",
    "postinstall": "patch-package"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "7.12.17",
    "@koa/cors": "2.2.1",
    "@nuxtjs/auth-next": "5.0.0-1617968180.f699074",
    "@nuxtjs/axios": "5.13.1",
    "@nuxtjs/proxy": "2.1.0",
    "@nuxtjs/router": "1.0.1",
    "area-data": "5.0.6",
    "axios": "0.17.1",
    "cropperjs": "1.2.2",
    "cross-env": "5.0.1",
    "crypto-js": "^4.1.1",
    "joi": "^17.9.2",
    "js-cookie": "2.2.0",
    "jsonwebtoken": "^9.0.1",
    "koa": "2.4.1",
    "koa-better-body": "^3.3.9",
    "koa-compose": "^4.1.0",
    "koa-joi-router": "^8.0.0",
    "koa-logger": "3.2.0",
    "koa-qs": "^3.0.0",
    "koa-session2": "1.0.10",
    "koa-static": "5.0.0",
    "nodemailer": "^6.9.4",
    "nuxt": "2.12.2",
    "pg": "^8.11.1",
    "pm2": "4.4.0",
    "rasterizehtml": "1.2.4",
    "sequelize": "^6.32.1",
    "simplemde": "1.11.2",
    "uuid": "^9.0.0",
    "view-design": "4.1.3",
    "vuex-persist": "1.2.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "ava": "0.25.0",
    "axios-mock-adapter": "1.15.0",
    "backpack-core": "0.8.4",
    "eslint": "4.3.0",
    "eslint-config-standard": "10.2.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-html": "3.1.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "jsdom": "11.11.0",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "nodemon": "1.11.0",
    "patch-package": "6.4.7",
    "quill-image-extend-module": "1.1.2",
    "scmp": "2.0.0",
    "vue-i18n": "7.6.0"
  }
}
